		if(isset($_REQUEST['suite_number'])) $tab_tour_cpt['SUITE_NUMBER'] = $_REQUEST['suite_number'];
		if(isset($_REQUEST['clear_height'])) $tab_tour_cpt['CLEAR_HEIGHT'] = $_REQUEST['clear_height'];
