			$tab_tour_eu['ID_SOUS_LOCATION'] = fix_bool($_REQUEST['sublease']);
		}
		if(isset($_REQUEST['negotiable'])) {
			$tab_tour_eu['ID_PRIX_NEGOCIABLE'] = fix_bool($_REQUEST['negotiable']);
		}
		if(isset($_REQUEST['ceil_h_unit'])) {
