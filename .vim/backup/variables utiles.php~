select * from video_diffusion where id_video in (
select distinct video.id_video from video 
left join video_conversion on video_conversion.id_Video=video.id_video
where id_visite in (
	select id_visite from (
		select id_visite,type_video,count(*) nb from video 
		group by id_visite,type_video
	) as tab where nb > 1
) and id_conversion is null
)
