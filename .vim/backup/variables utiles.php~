insert into diffusion_portail(id_diffusion_portail,id_portail,ref_portail,id_visite,date_diffusion,id_statut_diffusion)
select newid(),'A13B5F8E-A0B5-3CDA-4D7F-6CF5A25F02EB', id_visite, id_visite, getdate(), 0 from visite where id_utilisateur='5347D152-167A-A280-0DB9-4DA181B334D9' and id_statut_Visite > 0

// Bug video logo stup 
// 2012-07-26: 701
select id_video
from video_cron_suivi 
where date_conversion between '2012-07-10 15:15:00' and '2012-07-10 19:10:00'
and id_video not in (
	select id_video from video_cron_suivi where date_conversion > '2012-07-10 19:10:00'
)
 order by date_conversion asc

