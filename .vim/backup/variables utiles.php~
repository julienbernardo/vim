[STATS VIDEO]
select '28/09/2011',site_diffusion,id_statut_diffusion,result_code_diffusion,result_text_diffusion,result_description_diffusion,count(*) from video_diffusion
where date_modification_diffusion between '2011-09-28 00:00:00' and '2011-09-29 00:00:00'
group by site_diffusion,id_statut_diffusion,result_code_diffusion,result_text_diffusion,result_description_diffusion


select '28/09/2011', type_conversion, id_statut_conversion, result_text_conversion, count(*) from video_conversion
where date_demande_conversion between '2011-09-28 00:00:00' and '2011-09-29 00:00:00'
group by type_conversion, id_statut_conversion, result_text_conversion

select nom_distributeur, substring(CONVERT(varchar(10), Date_fin_conversion, 20),0,8), count(*) from video_conversion
inner join video on video.id_video=video_conversion.id_video
inner join distributeur on distributeur.id_distributeur=video.id_distributeur
where date_demande_conversion between '2011-11-01 00:00:00' and '2012-02-01 00:00:00'
and date_fin_conversion is not null
group by nom_distributeur,substring(CONVERT(varchar(10), Date_fin_conversion, 20),0,8)

