	case 'del_openhouse':
		if(del_openhouse((int)$_POST['oh'], $tour, $_SESSION['ID_UTILISATEUR'])) {
			echo 'true';
		}
		break;


	case 'open_del_openhouse':
?>
<h2><?=$dico['edit_tour']['confirm_delete_openhouse']?></h2>
<div class="navigation_btn">
