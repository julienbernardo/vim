					$diffusion = add_manual_publication($company, $partner, $tour, $ref, $multidiffusion, false, true);
					if(is_guid($diffusion['id']))  update_diffusion_portail_option($diffusion['id'], 'PUBLICATIONERRORCODE', $output);
