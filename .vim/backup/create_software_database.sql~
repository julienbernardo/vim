	Id_image uniqueidentifier NOT NULL,
	Modele_camera varchar(50) NULL,
	Marque_camera varchar(50) NULL,
	Md5_image char(32) NULL,
	CONSTRAINT [PK_IMAGE_INFOS] PRIMARY KEY CLUSTERED ( Id_image ASC )
)

